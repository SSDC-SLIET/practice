#include<iostream>
using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int a[n],s1=0,s2=0,i;
	    for(int i=0;i<n;i++)
	        cin>>a[i];
	   for(i=0;i<n/2;i++)
	     s1=s1+a[i];
	   for(i=n/2;i<n;i++)
	     s2=s2+a[i];
	cout<<s1*s2<<endl;   
	}
	return 0;
}
