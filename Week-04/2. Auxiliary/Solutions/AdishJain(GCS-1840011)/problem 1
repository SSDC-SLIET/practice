#include<iostream>

using namespace std;

int main()
 {
	
	int t;

	cin>>t;

	while(t--){

	    int i,j,n,k,m=0;

	    cin>>n>>k;

	    long int arr[n];

	    for(i=0;i<n;i++)

	        cin>>arr[i];

	    for(i=0;i<n;i++)

	        for(j=0;j<n;j++)

	        {

	            if(arr[i]%arr[j]==k)

	                m++;

	        }
	    
	     cout<<m<<endl;

	}

	return 0;

}
