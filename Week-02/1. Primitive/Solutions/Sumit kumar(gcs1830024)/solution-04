#include<iostream>
using namespace std;
int main()
 {
     int t,max,min;
     cin>>t;
     while(t)
     {
         int n;
         cin>>n;
         int ar[n];
         for(int i=0;i<n;i++)
         {
             cin>>ar[i];
         }
         max=ar[0];
         min=ar[0];
         for(int i=1;i<n;i++)
         {
             if(max<ar[i])
             {
                 max=ar[i];
             }
             if(min>ar[i])
             {
                 min=ar[i];
             }
         }
         cout<<max<<" "<<min<<endl;
         t--;
     }
	//code
	return 0;
}
