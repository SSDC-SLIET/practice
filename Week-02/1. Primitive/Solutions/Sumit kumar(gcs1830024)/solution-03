#include<iostream>
using namespace std;
int main()
 {
     int t,n;
     cin>>t;
     while(t)
     {
        cin>>n;
        int ar[n];
        for(int i=0;i<n;i++)
        {
            cin>>ar[i];
        }
        for(int i=n-1;i>=0;i--)
        {
            cout<<ar[i]<<" ";
        }
        cout<<endl;
        t--;
     }
	//code
	return 0;
}
