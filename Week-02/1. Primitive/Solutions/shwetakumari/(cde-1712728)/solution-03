#include <stdio.h>
#include <stdlib.h>

int main()
{
    int m,t;
    scanf("%d",&m);
    for(t=0;t<m;t++)
    {
        int n;
        scanf("%d",&n);
        int a[n],i;
        for(i=1;i<=n;i++)
        {
            scanf("%d",&a[i]);
        }
        for(i=n;i>0;i--)
        {
            printf("%d ",a[i]);
        }
printf("\n");
    }
    return 0;
}
