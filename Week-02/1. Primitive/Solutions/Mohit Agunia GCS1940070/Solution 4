#include <iostream>
using namespace std;

int main() {
	int t,n,max,min,x;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>n;
	    cin>>x;
	    max=x;
	    min=x;
	    for(int j=1;j<n;j++)
	    {
	        cin>>x;
	        if(x>max)
	        max=x;
	        if(x<min)
	        min=x;
	    }
	    cout<<max<<" "<<min<<endl;
	}
	return 0;
}
