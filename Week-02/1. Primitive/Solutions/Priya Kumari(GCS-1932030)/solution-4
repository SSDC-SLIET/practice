#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
   for(int i=1;i<=t;i++)
    {
        int n;
        cin>>n;
        int a[n],max,min;
        for(int i=0;i<n;i++)
          {
            cin>>a[i];
          }
        sort(a,a+n);
        max=a[n-1];
        min=a[0];
         cout<<max<<" "<<min<<endl;
    }
     return 0;
}
