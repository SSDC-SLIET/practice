#include<iostream>
using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n,i,j,temp=0;
	    cin>>n;
	    int a[n][n];
	 for(i=0;i<n;i++)
	 {
	 for(j=0;j<n;j++)
	 {
	 cin>>a[i][j];
	 }
	 }
	 for(i=0;i<n;i++)
	 {
	     for(j=n-1;j>=0;j--)
	     {
	      cout<<a[j][i]<<" ";  
	     }
	 }
	 cout<<endl;
	}
	return 0;
}
