#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    int n;
	    cin>>n;
	    int a[n];
	    for(int j=0;j<n;j++)
	    {
	        cin>>a[j];
	    }
	    for(int j=0;j<n;j=j+2)
	    {
	        if(a[j]==a[j+1])
	        {
	            continue;
	        }
	        else
	        {
	            cout<<a[j]<<endl;
	            break; 
	        }
	    }
	}
	return 0;
}
