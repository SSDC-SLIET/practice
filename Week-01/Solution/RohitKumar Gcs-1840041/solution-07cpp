#include <iostream>
using namespace std;


int main() {
	//code
	int t,i,j;
	cin>>t;
	for(i=0;i<t;i++)
	{
	    int n;
	    cin>>n;
	    int a[n][n];
	    for(int k=0;k<n;k++)
	    {
	        for(j=0;j<n;j++)
	        {
	          cin>>a[k][j];  
	        }
	    }
	    for(j=0;j<n;j++)
	    {
	        for(int k=n-1;k>=0;k--)
	        {
	            cout<<a[k][j]<<" ";
	        }
	    }
	    cout<<endl;
	}
	return 0;
}
