#include<iostream>
using namespace std;
int main()
 {
     int t;
     cin>>t;
     while(t--)
     {
         int n,a,b,c=0,t;
         cin>>n>>a>>b;
         int ar[n];
         for(int i=0;i<n;i++)
         {
             cin>>ar[i];
         }
         for(int j=a;j<=b;j++)
         {
             for(int i=0;i<n;i++)
             {
                 if(ar[i]==j)
                 {
                    c++;
                 }
             }
         }
         t=b-a+1;
         if(c==t)
            cout<<"Yes"<<endl;
        else
            cout<<"No"<<endl;
     }
	return 0;
}
