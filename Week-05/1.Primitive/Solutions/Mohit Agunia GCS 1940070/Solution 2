#include <iostream>
using namespace std;

int main() {
	int t,n,i,mod,div;
	cin>>t;
	for(i=0;i<t;i++)
	{
	    cin>>n;
	    mod=n%3;
	    if(mod==0)
	    {
	        div=n/3;
	        cout<<div-1<<" "<<div<<" "<<div+1<<endl;
	    }
	    else
	    {
	        cout<<"-1"<<endl;
	    }
	    
	}
	return 0;
}
